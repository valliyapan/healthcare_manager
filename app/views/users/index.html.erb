<div class="block">
  <section>
    <h3>Users List</h3>
    <% User.all.each do |user| %>
      <p>Name : <%= user.name %></p>
      <p>Email : <%= user.email %></p>
      <p>Gender : <%= user.gender %></p>
      <p>Role : <%= user.role %></p>
      <%= form_with url: "/users/#{user.id}", method: :get do |form| %>
        <%= form.submit :View %>
      <% end %>
      <%= form_with url: "/users/#{user.id}", method: :delete do |form| %>
        <%= form.submit :Delete %>
      <% end %>
      <hr>
    <% end %>
  </section>
</div>
